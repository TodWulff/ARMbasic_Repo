#ifndef ABRTOS_INIT_SUPPORT
#define ABRTOS_INIT_SUPPORT

#define ABRTOS_MOD		ABRTOS_LOG_MOD_INIT
 
#define	ABRTOS_INIT_STARTUP_STARTED			0
#define	ABRTOS_INIT_STARTUP_COMPLETE		1

SUB ABRTOS_INIT_STARTUP
	
	#if ABRTOS_OPTION_USE_LOGGING == 1
		ABRTOS_LOG_INIT
	#endif
	
	ABRTOS_INT_SWI_SETUP
	
	ABRTOS_TASK_ACTIVEID = -1
	
		
	#if ABRTOS_OPTION_USE_LOGGING == 1
		ABRTOS_LOG_EVENT(ABRTOS_MOD,ABRTOS_INIT_STARTUP_STARTED)
	#endif
	
	ABRTOS_SEMA_STARTUP_INIT

	ABRTOS_WDT_STARTUP_CHECK
	ABRTOS_WDT_SETUP
	
		
	#if ABRTOS_OPTION_USE_LOGGING == 1
		ABRTOS_LOG_EVENT(ABRTOS_MOD,ABRTOS_INIT_STARTUP_COMPLETE)
	#endif
	
ENDSUB

#endif
